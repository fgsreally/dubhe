  },
  "dependencies": {
    "dubhe": "workspace:^",
    "dubhe-sub": "workspace:*",
    "element-plus": "^2.3.5",
    "http-server": "^14.1.1",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.1.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vue-tsc": "^1.4.2"
  }
}
