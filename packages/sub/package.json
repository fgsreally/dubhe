{
  "name": "dubhe-sub",
  "version": "2.0.1",
  "description": "Micro module subscription side",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup && yarn build:view",
    "build:view": "vite build"
  },
  "keywords": [
    "module federation",
    "micro module",
    "vite",
    "webpack"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/",
      "require": "./dist/"
    },
    "./vite": {
      "import": "./dist/vite.mjs",
      "require": "./dist/vite.js"
    },
    "./webpack": {
      "import": "./dist/webpack.mjs",
      "require": "./dist/webpack.js"
    },
    "./client": {
      "import": "./dist/client.mjs",
      "require": "./dist/client.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "dist/index.d.ts"
      ],
      "vite": [
        "dist/vite.d.ts"
      ],
      "webpack": [
        "dist/webpack.d.ts"
      ],
      "client": [
        "dist/client.d.ts"
      ]
    }
  },
  "author": "fgsreally",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.27.2",
    "colors": "^1.4.0",
    "debug": "^4.3.4",
    "dubhe": "workspace:*",
    "sirv": "^2.0.2",
    "vite": "^3.1.0",
    "webpack": "^5.0.0",
    "webpack-virtual-modules": "^0.5.0"
  },
  "peerDependencies": {
    "html-webpack-plugin": "^5.5.0",
    "webpack": "^5.0.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.40",
    "esbuild": "^0.16.9",
    "graphology": "^0.25.1",
    "html-webpack-plugin": "^5.5.0",
    "rollup": "2.78.1",
    "sigma": "^2.4.0",
    "tsup": "^6.1.2",
    "typescript": "^4.9.5",
    "unplugin-vue-components": "^0.22.9"
  }
}
