{
  "name": "sub-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "hot": "cross-env HOTBUILD=true vite build && npx dubhe bundle -o dist/hot/core -d dist/hot/core",
    "cold": " vite build && npx dubhe bundle -o dist/cold/core -d dist/cold/core",

    "preview": "vite preview",
    "serve": "http-server . --port=8082 --cors"
  },
  "dependencies": {
    "@vitejs/plugin-legacy": "^4.0.1",
    "dubhe": "workspace:^",
    "dubhe-pub": "workspace:*",
    "dubhe-sub": "workspace:*",
    "element-plus": "^2.2.29",
    "http-server": "^14.1.1",
    "rollup-plugin-visualizer": "^5.9.0",
    "unplugin-auto-import": "^0.14.2",
    "unplugin-vue-components": "^0.22.9",
    "vite-plugin-inspect": "^0.7.15",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vue-tsc": "^1.0.24"
  }
}
