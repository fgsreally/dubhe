{
  "name": "dubhe-repo",
  "version": "1.0.0",
  "description": "",
  "packageManager": "pnpm@7.21.0",

  "scripts": {
    "dev": "turbo run dev --filter dubhe-* --filter dubhe",
    "build": "turbo run build --filter dubhe-* --filter dubhe",
    "publish": "changeset publish",
    "lint": "eslint . --ext .ts",
    "test": "cross-env TEST=true DEBUG=dubhe* node test/before-test.mjs",
    "test:e2e": "playwright test e2e/",
    "test:unit": "vitest --run",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",  
    "example:serve": "pnpm -r --parallel run serve ",
    "example:pub": "pnpm --filter=pub-* run pub ",
    "example:sub": "pnpm --filter=sub-* run dev",
    "example:cold": "pnpm --filter sub-vuecli --filter sub-vite run cold",
    "example:hot": "pnpm --filter sub-vuecli --filter sub-vite run hot"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@antfu/eslint-config": "^0.31.0",
    "@changesets/cli": "^2.25.2",
    "@playwright/test": "^1.31.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.28.0",
    "fast-glob": "^3.2.12",
    "puppeteer": "^19.7.3",
    "turbo": "^1.6.3",
    "vitepress": "1.0.0-alpha.49",
    "vitest": "^0.25.6",
    "wait-on": "^7.0.1",
    "zx": "^7.2.1"
  }
}