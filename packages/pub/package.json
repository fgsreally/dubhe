{
  "name": "dubhe-pub",
  "version": "2.0.0-alpha.0",
  "description": "Micro module publishing side",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "build:view": "vite build"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "module federation",
    "micro module",
    "vite",
    "esbuild"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/",
      "require": "./dist/"
    },
    "./vite": {
      "import": "./dist/vite.mjs",
      "require": "./dist/vite.js"
    },
    "./esbuild": {
      "import": "./dist/esbuild.mjs",
      "require": "./dist/esbuild.js"
    }
  },
  "typesVersions": {
    "*": {
      "vite": [
        "dist/vite.d.ts"
      ],
      "esbuild": [
        "dist/esbuild.d.ts"
      ]
    }
  },
  "author": "fgsreally",
  "license": "MIT",
  "dependencies": {
    "@vue/compiler-sfc": "^3.2.45",
    "debug": "^4.3.4",
    "dubhe": "workspace:^2.0.0-alpha.0",
    "es-module-lexer": "^0.10.5",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.1.0",
    "magic-string": "^0.26.4",
    "ts-morph": "^17.0.1",
    "typescript": "^4.5.4",
    "unplugin": "^1.0.0",
    "vite": "^3.1.0",
    "vite-plugin-dynamic-base": "^0.4.8",
    "webpack": "^5.75.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.40",
    "esbuild": "^0.16.9",
    "esbuild-plugin-merge": "^1.0.6",
    "rollup": "2.78.1",
    "tsup": "^6.1.2"
  }
}
